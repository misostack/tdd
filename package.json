{
  "name": "tdd",
  "version": "1.0.0",
  "description": "test driven development guidelines",
  "main": "dist/main.js",
  "scripts": {
    "start": "node ./dist/main.js",
    "start:debug": "ts-node-dev -r tsconfig-paths/register --transpile-only --exit-child --inspect=4321 --project tsconfig.dev.json src/main.ts",
    "start:dev": "ts-node-dev -r tsconfig-paths/register --transpile-only  --exit-child --project tsconfig.dev.json",
    "build": "tsc -p tsconfig.production.json",
    "test": "jest --runTestsByPath"
  },
  "keywords": [
    "tdd",
    "typescript",
    "jsbase",
    "nextjsvietnam"
  ],
  "author": "jsbase",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.7",
    "jest": "^29.6.4",
    "jest-junit": "^16.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@tsconfig/node16": "^16.1.1",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.8.0"
  }
}
